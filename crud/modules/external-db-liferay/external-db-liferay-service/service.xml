<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="com.exter" auto-namespace-tables="false">
	<namespace>APPLICATION</namespace>
	<entity local-service="true" name="Vlog" remote-service="false" uuid="true" table="vlog" data-source="extDataSource">

		<column name="vlogId" type="long" primary="true"></column>
        <column name="vlogName" type="String"></column>
	</entity>
</service-builder>